{
  "name": "@oppulence-engineering/reacher-sdk",
  "version": "0.2.0",
  "description": "TypeScript SDK for Reacher Email Verification API",
  "main": "./dist/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc -p tsconfig.build.json",
    "build:esm": "tsc -p tsconfig.build.json --module ESNext --outDir dist/esm",
    "clean": "rm -rf dist",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "jest",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "dependencies": {
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.3.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0",
    "jest": "^29.0.0",
    "@types/jest": "^29.0.0",
    "ts-jest": "^29.0.0"
  },
  "keywords": [
    "reacher",
    "email",
    "verification",
    "validation",
    "api",
    "sdk"
  ],
  "author": "Oppulence Engineering",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Oppulence-Engineering/check-if-email-exists.git",
    "directory": "sdks/typescript"
  },
  "bugs": {
    "url": "https://github.com/Oppulence-Engineering/check-if-email-exists/issues"
  },
  "homepage": "https://github.com/Oppulence-Engineering/check-if-email-exists/tree/master/sdks/typescript#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
